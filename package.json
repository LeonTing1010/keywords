{
  "name": "keywordnova",
  "version": "1.0.0",
  "main": "keywordNova.ts",
  "scripts": {
    "keywords": "ts-node keywordNova.ts",
    "debug": "node --inspect-brk -r ts-node/register keywordNova.ts",
    "debug:google": "node --inspect-brk -r ts-node/register keywordNova.ts test --engine google --temp-browser",
    "build": "tsc",
    "dev": "ts-node keywordNova.ts",
    "debug:tools": "./debug-tools.sh",
    "debug:runner": "DEBUG=true ts-node src/tools/debug-runner.ts",
    "debug:network": "DEBUG=true ts-node src/tools/debug-runner.ts network",
    "simple": "ts-node keywordNova.ts --no-llm",
    "analyze": "ts-node keywordNova.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "seo",
    "keywords",
    "long-tail",
    "autocomplete",
    "search-suggestions",
    "intent-mining",
    "content-strategy"
  ],
  "author": "",
  "license": "ISC",
  "description": "KeywordNova - 意图挖掘与长尾关键词爆破系统",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/axios": "^0.14.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "axios": "^1.6.7",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "bin": {
    "keywordnova": "./keywordNova.ts"
  }
}
