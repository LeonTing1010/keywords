{
  "name": "keywordnova",
  "version": "1.0.0",
  "main": "keywordsTool.ts",
  "scripts": {
    "keywords": "ts-node keywordsTool.ts",
    "debug": "node --inspect-brk -r ts-node/register keywordsTool.ts",
    "debug:google": "node --inspect-brk -r ts-node/register keywordsTool.ts test --engine google --temp-browser",
    "build": "tsc",
    "dev": "ts-node keywordsTool.ts",
    "debug:tools": "./debug-tools.sh",
    "debug:runner": "DEBUG=true ts-node src/tools/debug-runner.ts",
    "debug:network": "DEBUG=true ts-node src/tools/debug-runner.ts network",
    "debug:secondary": "DEBUG=true ts-node src/tools/debug-secondaryQuery.ts",
    "iterative": "ts-node keywordsTool.ts --iterative",
    "analyze": "ts-node keywordsTool.ts --use-llm"
  },
  "keywords": [
    "seo",
    "keywords",
    "long-tail",
    "autocomplete",
    "search-suggestions",
    "intent-mining",
    "content-strategy"
  ],
  "author": "",
  "license": "ISC",
  "description": "KeywordNova - 意图挖掘与长尾关键词爆破系统",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/axios": "^0.14.0",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^22.15.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "axios": "^1.6.7",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "bin": {
    "keywordnova": "./keywordsTool.ts"
  }
}
